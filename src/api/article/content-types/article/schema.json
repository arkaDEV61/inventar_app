{
  "kind": "collectionType",
  "collectionName": "articles",
  "info": {
    "singularName": "article",
    "pluralName": "articles",
    "displayName": "Article",
    "description": "Create your blog content"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::category.category",
      "inversedBy": "articles"
    },
    "invoices": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::invoice.invoice",
      "mappedBy": "article"
    },
    "amount": {
      "type": "string"
    },
    "article_unit": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::article-unit.article-unit",
      "inversedBy": "articles"
    },
    "updated_by_user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "articles"
    },
    "amount_old": {
      "type": "string"
    },
    "difference": {
      "type": "decimal"
    },
    "threshold": {
      "type": "decimal"
    }
  }
}
